<h1>New promo_generale</h1>

<% form_for(@promo_generale) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :titre %><br />
    <%= f.text_field :titre %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :date_debut %><br />
    <%= f.date_select :date_debut %>
  </p>
  <p>
    <%= f.label :date_fin %><br />
    <%= f.date_select :date_fin %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', promo_generales_path %>